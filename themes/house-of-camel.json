      {
        "scope": ["entity.name.module.js", "variable.import.parameter.js", "variable.other.class.js"],
        "settings": {
          "foreground": "#FFFACD"
        }
      },
      {
        "scope": ["variable.language"],
        "settings": {
          "foreground": "#BDBDBD",
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["markup.italic", "markup.strike"],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["markup.bold", "markup.bold string"],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["*url*", "*link*", "*uri*"],
        "settings": {
          "foreground": "#BDBDBD",
          "fontStyle": "underline"
        }
      }
    ]
